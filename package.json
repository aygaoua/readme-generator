  "description": "A smart CLI tool that generates professional README.md files by auto-detecting project info from package.json and git config.",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
