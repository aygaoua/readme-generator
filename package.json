    "index.d.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aygaoua/readme-generator.git"
  },
  "bugs": {
    "url": "https://github.com/aygaoua/readme-generator/issues"
  },
  "homepage": "https://github.com/aygaoua/readme-generator#readme",
